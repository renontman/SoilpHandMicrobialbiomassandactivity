modelNO3BBlin <- lm( NO3.BB$NO3 ~ NO3.BB$pH)
modelNO3W1lin<- lm( NO3.W1$NO3 ~ NO3.W1$pH)
summary(modelNO3BBlin)
summary(modelNO3W1lin)

